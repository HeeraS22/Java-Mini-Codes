package com.Heera.JavaBasicPrograms;

import java.util.ArrayList;

import com.Qspiders.Collection.Laptop;

public class Arraylist2 
{
	public static void main(String[] args) 
	{
		ArrayList<Laptop> al = new ArrayList<Laptop>();
		al.add(new Laptop(4,"Dell"));
		al.add(new Laptop(6, "HP"));
		al.add(new Laptop(16, "HP"));
		al.add(new Laptop(8,"Dell"));
		

		for(int i=0;i<al.size();i++)
		{
			Object o = al.get(i);
			if(o instanceof Laptop)
			{
				Laptop l = (Laptop)o;
				if(l.getBrand().equals("Dell"))
				{
					System.out.println(l);
				}
			}
		}		
	}
}
