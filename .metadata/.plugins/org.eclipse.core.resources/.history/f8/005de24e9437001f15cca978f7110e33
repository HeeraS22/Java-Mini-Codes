package com.Heera.JavaBasicPrograms;

public class String2 
{
	public static void main(String[] args) 
	{
		String s1 ="My fav dessert is Gulab jamun";
		String s2 =""; //empty string
		
		for(int i=0;i<s1.length();i++)
		{
			if(s1.charAt(i)!=' ')
				s2 = s2+s1.charAt(i);
		}
		
		System.out.println(s2); //MyfavdessertisGulabjamun
	}
}
