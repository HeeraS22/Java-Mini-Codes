package com.Heera.JavaBasicPrograms;

public class NeonNumber {

	public static void main(String[] args) {
		int num =9;
		int sq = num*num;
		int sum =0;
		
		while(sq>0)
		{
			int rem = sq%10;
			sum = sum+rem;
			sq = sq/10;
		}
		if(num==sum)
			System.out.println("Neon number");
		else
			System.out.println("Not a neon number");
	}

}
